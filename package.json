{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Home of my Full Stack Software Developer portfolio, made with React.js and PostCSS.",
  "main": "index.js",
  "engines": {
    "node": "14.15.4",
    "npm": "6.14.10"
  },
  "scripts": {
    "clean": "rm -rf node_modules && npm i",
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "jest --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watch --updateSnapshot",
    "start": "webpack-dev-server --hot --mode development --devtool eval-source-map --open",
    "deploy:preview": "npm run build && serve -s dist",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caripizza/portfolio.git"
  },
  "keywords": [],
  "author": "Cari Palazzolo",
  "license": "ISC",
  "homepage": "https://github.com/caripizza/portfolio#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "jsdom"
  },
  "dependencies": {
    "better-react-spinkit": "^2.0.4",
    "react": "^16.14.0",
    "react-burger-menu": "^2.9.2",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^3.0.1",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.2.0",
    "jsdom": "^20.0.1",
    "mini-css-extract-plugin": "^1.3.5",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.3",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.14.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3"
  }
}
