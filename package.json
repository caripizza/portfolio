{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "Home of my Full Stack Software Developer portfolio, made with React.js and PostCSS.",
  "main": "index.js",
  "engines": {
    "node": "13.9.0",
    "npm": "6.13.7"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest --verbose --passWithNoTests",
    "test:watch": "npm run test -- --watch --updateSnapshot",
    "start": "webpack-dev-server --hot --mode development --devtool eval-source-map",
    "start-network": "webpack-dev-server --hot --mode development --devtool eval-source-map --host 0.0.0.0",
    "build": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caripizza/portfolio.git"
  },
  "keywords": [],
  "author": "Cari Palazzolo",
  "license": "ISC",
  "homepage": "https://github.com/caripizza/portfolio#readme",
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "better-react-spinkit": "^2.0.4",
    "promise-middleware-redux": "^1.2.0",
    "react": "^16.13.1",
    "react-burger-menu": "^2.6.13",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
